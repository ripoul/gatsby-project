{
  "name": "gatsby-starter",
  "private": true,
  "description": "A simplified bare-bones starter for Gatsby",
  "version": "1.0.0",
  "license": "MIT",
  "scripts": {
    "build": "gatsby build",
    "develop": "gatsby develop",
